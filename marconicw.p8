-- Intro
function write_init()
    
    char_index = 0
    frame_counter = 0
    current_text = ""
    text_ready = false
    text_zeile += 1
    if text_zeile > #texte then
        intro=false
        return
    else
      text=texte[text_zeile]
    end
end

function text_init()
    texte={"die reichweite habe ich jetzt"}
add(texte,"im griff. ich schaffe es jetzt") 
add(texte,"mehr als einen kilometer")
add(texte,"zu ueberbruecken.")
add(texte,"aber nur eine glocke klingeln") 
add(texte,"zu lassen hilft ehrlich gesagt")
add(texte,"wenig. ich muss es schaffen,")
add(texte,"morsezeichen zu senden.")
add(texte,"dafuer habe ich einen")
add(texte,"morseschreiber konstruiert,")
add(texte,"der selbststaendig") 
add(texte,"texte auf ein")
add(texte,"papierband schreibt.") 
add(texte,"um das zu testen will ich")
add(texte,"das wort morse senden.")
add(texte,"dazu muss ich die buchstaben")
add(texte,"richtig anordnen und die") 
add(texte,"sendetaste druecken.")
delay_frames = 2 -- Wie viele Frames auf einen Buchstaben gewartet wird
text_zeile = 0
end

function write(posx, posy)
    if btnp(üÖæÔ∏è) then intro=false end
    frame_counter+=1
    if (btn(‚ùé)) char_index=#text-1
    if frame_counter >= delay_frames  then
    frame_counter = 0
      if char_index < #text then
        char_index += 1
        current_text = sub(text, 1, char_index)
        sfx(0)
      else
        text_ready = true
        frame_counter = 0  
    end
    end
    print(current_text, posx, posy, 7) 
    
end

function wait(zeit,posx,posy)
 frame_counter+=1
 if frame_counter >= zeit then
    frame_counter = 0
    return true
 else
    print(current_text, posx, posy, 7)
    return false
 end
end

function intro_draw()
    
    
    if not text_ready then 
    write(1, 10)
else
    if wait(10,2,10) then
     write_init()
    end  
end

if frame_counter==0 then
   spr(2,34,20,1,2)
else
   spr(1,34,20,1,2)
end

end


