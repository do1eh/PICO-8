--game

function game_init()
 player={x=24,y=89,dir='o'}
 play={a=1,b=2,c=3,d=4,e=5,f=6,g=7,h=8,i=9,j=10,k=11,l=12,m=13,n=14,o=15,p=16,q=17,r=18,s=19,t=20,u=21,v=22,w=23,x=24,y=25,z=26}
 blockoben= {'h', 'e', 'l', 'o', 'n', 'x', 't', 'w', 'k', 'i', 'f', 'r', 'a', 'z'}
 blockunten={'g', 'b', 'd', 'u', 'p', 's', 'y', 'q', 'j', 'm', 'v', 'c', 'n', 'x'}
 senden={}
 sendtext=''
 paper_counter=1
 animatepaper=false
 win=false
end





function game_update()
     
    frame_counter+=1
 if frame_counter==100 then
    frame_counter=0
 end
    paper_counter+=1
 if paper_counter==4 then
    paper_counter=0
 end
 gewonnen()
 move()
 
end

function gewonnen()
 gesendet='';
  for element in all(senden) do
      gesendet=gesendet..element
  end
  if gesendet=='morse' then
        win=true
end
end

function move()
  if not animatepaper then  
    if btnp(‚¨ÜÔ∏è) then 
        
        player.dir='n'
        sfx(play[blockoben[player.x/8]])
        
    end
    if btnp(‚¨áÔ∏è) then 
        
        player.dir='s'
         sfx(play[blockunten[player.x/8]])
    end
    if btnp(‚¨ÖÔ∏è) then 
        if player.x > 8 then
            player.x-=8
            if frame_counter%6==0 then sfx(0) end
        end
        player.dir='o'
    end
    if btnp(‚û°Ô∏è) then 
        if player.x < 112 then
            player.x+=8
            if frame_counter%6==0 then sfx(0) end
        end
        player.dir='w'
    end
    if btnp(‚ùé) then 
        buchstabeo=blockoben[player.x/8]
        buchstabeu=blockunten[player.x/8]
        if  player.dir=='n' then 
            --print(blockoben[player.x/8])
            add(senden,buchstabeo) 
          elseif player.dir=='s' then 
            add(senden,buchstabeu) 
        end
        if (#senden==1) mset(3,7,58) 
        if (#senden==2) mset(5,7,58)
        if (#senden==3) mset(7,7,58)
        if (#senden==4) mset(9,7,58)
        if #senden==5 then
            mset(11,7,58)
            frame_counter=0
        end
    end
  end 
end


function animatesenden()
  if #senden==5 then
     sendtext=''
     for element in all(senden) do
         sendtext=sendtext..element..' '
     end
     ticker(108,18)
  end
end

function ticker(posx, posy)
    frame_counter+=1
    
    if frame_counter >= 2  then
    frame_counter = 0
    char_index += 1
      if char_index < #sendtext then
        current_text = sub(sendtext, 1, char_index)
        animatepaper=true
      else
        text_ready = true
        frame_counter = 0  
    end
    end
    posx=posx-char_index
    --papier animieren
    if paper_counter<1 then
       for i=2,14 do
          mset(i,2,8)
       end
    elseif paper_counter<2 then
         for i=2,14 do
             mset(i,2,9)
         end 
    elseif paper_counter<3 then
         for i=2,14 do
             mset(i,2,10)
         end
    elseif paper_counter<4 then
         for i=2,14 do
             mset(i,2,11)
         end
         
       
    end
    --text drucken
    print(current_text, posx, posy, 0)
    spr(32,15*8,2*8) 
    spr(32,0,2*8)
    if posx<-50 then
        gewonnen()
        senden={}
        sendtext=''
        char_index=0
        paper_counter=1
        animatepaper=false
        mset(3,7,48) 
        mset(5,7,48)
        mset(7,7,48)
        mset(9,7,48)
        mset(11,7,48)
    end
end

function game_draw()
map()
if(player.dir=='n') spr(5,player.x,player.y,1,2)
if(player.dir=='s') spr(1,player.x,player.y,1,2)
if(player.dir=='w') spr(3,player.x,player.y,1,2,true,false)
if(player.dir=='o') spr(3,player.x,player.y,1,2)

animatesenden()
--gewonnen
if win then
