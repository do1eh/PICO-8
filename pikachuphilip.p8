pico-8 cartridge // http://www.pico-8.com
version 43
__lua__
function _init()
 x = 2
 y = 40
 spiegeln=false
 
 speed = 0
 gravitation=0.4
 sprungkraft=-4
 gameover=false

 end

 function _update()
 speed += gravitation
 y += speed
 
 
 
 if is_tot(x + 1, y + 8) or is_tot(x + 6, y + 8) then
 gameover=true
 end

 if speed > 0 then
       if is_solid(x + 1, y + 8) or is_solid(x + 6, y + 8) then
         local tile_y = flr((y + 7) / 8)
         y = tile_y * 8 
         speed = 0
      end
  end

if btnp(⬆️) and speed == 0 and is_solid(x + 1, y + 8) then 
    speed = sprungkraft
  end


if btn(➡️) then
   local block=mget(flr((x+8)/8),flr(y/8))
   if not fget(block,0) then
   x=x+1
   end
   spiegeln=false
end

if btn(⬅️) then
   x=x-1 
   spiegeln=true
end
     

end

function _draw()
cls()

if gameover then 
   map()
   spr(1,x,y,1,1,spiegeln)
end
print(gameover,10,10)
end
-->8
function is_solid(px, py)
    local tile_x = flr(px / 8)
    local tile_y = flr((py + 7) / 8) 
    local sprite_id = mget(tile_x, tile_y)
   return fget(sprite_id, 0)
end

function is_tot(px, py)
    local tile_x = flr(px / 8)
    local tile_y = flr((py + 7) / 8) 
    local sprite_id = mget(tile_x, tile_y)
   return fget(sprite_id, 1)
end

