--hund
function movedog()
  if (dogstep%2==0) then
     dogspr=4
  else
     dogspr=5
  end      
  spr(dogspr,hx+dogstep,hy,1,1,dogdir)   
  if (dogdir==false) then
    dogstep=dogstep+1
    if (dogstep==85) dogdir=true
  else
   dogstep=dogstep-1
   if (dogstep==0) dogdir=false   
  end
  zufall=flr(rnd(100)) + 1
  if  (zufall == 10) dogdir=not dogdir
end
