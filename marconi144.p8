-- Intro
function write_init()
    
    char_index = 0
    frame_counter = 0
    frame_counter2 = 0
    current_text = ""
    text_ready = false
    text_zeile += 1
    if text_zeile > #texte then
        intro=false
        return
    else
      text=texte[text_zeile]
    end
end

