-- stern klasse
stern = {}

function stern:new(winkel, abstand)
    local o = {}
    setmetatable(o, self)
    self.__index = self
    o.winkel = winkel 
    o.abstand = abstand
    o.farbe=flr(rnd(16)) + 1 
    return o
end

function stern:update()
    self.abstand=self.abstand+1
end

function stern:draw()
    local center_x = 64
    local center_y = 64

    local x = center_x + self.abstand * cos(self.winkel)
    local y = center_y + self.abstand * sin(self.winkel)
    farbe=flr(rnd(16)) + 1
    --feuerwerk
    pset(x, y, farbe) 
    --konfetti
    --pset(x, y, self.farbe) 
end
