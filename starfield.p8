function _init()
     sterne = {}
     maxabstand=80
     pi=3.141
 end

 function _update()
     --neue sterne spawnen
     local zufalls_winkel = rnd(2 * pi) -- winkel in radiant von 0 bis 2*pi
     add(sterne, stern:new(zufalls_winkel, 10))
     --alle sterne durchlaufen
     for stern in all(sterne) do
         if (stern.abstand>maxabstand) del(sterne,stern)
         stern:update()
         
     end
 end

 function _draw()
     cls() 
     for stern in all(sterne) do
         stern:draw()
     end
 end
